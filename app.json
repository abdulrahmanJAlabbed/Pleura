{
  "expo": {
    "name": "Pleura",
    "slug": "Pleura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon/ms-icon-310x310.png",
    "scheme": "pleura",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/favicon/apple-icon-180x180.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/favicon/android-icon-192x192.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/favicon/android-icon-310x310.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "black",
          "dark": {
            "backgroundColor": "black"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
